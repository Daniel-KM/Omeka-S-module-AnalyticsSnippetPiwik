{
    "name": "daniel-km/Omeka-S-module-AnalyticsSnippetPiwik",
    "description": "Omeka S submodule for Analytics Snippet to track json (in particular Omeka S API) and xml (like with OAI-PMH Repository) calls with Piwik.",
    "license": " CECILL-2.1",
    "require-dev": {
        "phpunit/phpunit": "~5.4",
        "friendsofphp/php-cs-fixer": "^2.0",
        "composer/composer": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "AnalyticsSnippetPiwik\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AnalyticsSnippetPiwikTest\\": "test/AnalyticsSnippetPiwikTest/"
        }
    },
    "extra": {
        "install-name": "AnalyticsSnippetPiwik"
    },
    "require": {
        "piwik/piwik-php-tracker": "^1.3"
    }
}
